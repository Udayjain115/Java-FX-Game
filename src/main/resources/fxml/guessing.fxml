<Pane prefHeight="562.0" prefWidth="1008.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nz.ac.auckland.se206.controllers.GuessingController">
   <children>
      <ImageView fitHeight="446.0" fitWidth="774.0" layoutY="5.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/guessingIMG.png" />
         </image>
      </ImageView>
      <TextField fx:id="textInput" layoutX="791.0" layoutY="410.0" prefHeight="25.0" prefWidth="158.0" />
      <Button fx:id="btnSend" layoutX="951.0" layoutY="410.0" mnemonicParsing="false" onAction="#onSendMessage" text="Send" />
      <Rectangle fx:id="police" arcHeight="5.0" arcWidth="5.0" fill="#1f93ff00" height="435.0" layoutY="5.0" onMouseClicked="#handleRectangleClicked" stroke="TRANSPARENT" strokeType="INSIDE" width="237.0" />
      <Rectangle fx:id="manager" arcHeight="5.0" arcWidth="5.0" fill="#1f93ff00" height="435.0" layoutX="237.0" layoutY="6.0" onMouseClicked="#handleRectangleClicked" stroke="TRANSPARENT" strokeType="INSIDE" width="285.0" />
      <Rectangle fx:id="janitor" arcHeight="5.0" arcWidth="5.0" fill="#1f93ff00" height="435.0" layoutX="522.0" layoutY="6.0" onMouseClicked="#handleRectangleClicked" stroke="TRANSPARENT" strokeType="INSIDE" width="253.0" />
      <TextArea fx:id="text" editable="false" layoutX="791.0" layoutY="14.0" prefHeight="382.0" prefWidth="201.0" wrapText="true" />
      <Label fx:id="timerLbl" alignment="CENTER" layoutX="884.0" layoutY="495.0" prefHeight="45.0" prefWidth="108.0" style="-fx-background-color: gray;" text="Label" />
      <ImageView fx:id="wrongPerson" fitHeight="435.0" fitWidth="774.0" layoutX="6.0" layoutY="6.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/WrongPerson.png" />
         </image>
      </ImageView>
      <ImageView fx:id="wrongReason" fitHeight="435.0" fitWidth="774.0" layoutX="6.0" layoutY="6.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/WrongReason.png" />
         </image>
      </ImageView>
      <ImageView fx:id="won" fitHeight="442.0" fitWidth="835.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/Won.png" />
         </image>
      </ImageView>
      <ImageView fx:id="timeOut" fitHeight="442.0" fitWidth="852.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/TimeOut.png" />
         </image>
      </ImageView>
   </children>
</Pane>
