                  <Image url="@../images/610-GJaV33L-removebg-preview.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="89.0" fitWidth="128.0" layoutX="207.0" layoutY="439.0">
               <image>
                  <Image url="@../images/Throwbookopen2-removebg-preview.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="104.0" fitWidth="150.0" layoutX="405.0" layoutY="470.0">
               <image>
                  <Image url="@../images/ZN177-FRONT-removebg-preview.png" />
               </image>
            </ImageView>
            <Rectangle fx:id="cameraRectangle" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="57.0" layoutX="251.0" layoutY="55.0" onMouseClicked="#cameraClick" opacity="0.0" stroke="BLACK" strokeType="INSIDE" width="75.0" />
            <Rectangle fx:id="rulebookRectangle" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="77.0" layoutX="214.0" layoutY="446.0" onMouseClicked="#openRuleBook" opacity="0.0" stroke="BLACK" strokeType="INSIDE" width="114.0" />
