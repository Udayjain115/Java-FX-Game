            <Rectangle fx:id="rulesBackground" arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="409.0" layoutX="271.0" layoutY="55.0" opacity="0.0" stroke="BLACK" strokeType="INSIDE" width="526.0" />
            <Text fx:id="rulesText" layoutX="512.0" layoutY="75.0" opacity="0.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Rules" />
            <Rectangle fx:id="rulesCloseBackground" arcHeight="5.0" arcWidth="5.0" fill="#a5b9cc" height="25.0" layoutX="755.0" layoutY="78.0" onMouseClicked="#closeRuleBook" opacity="0.0" stroke="BLACK" strokeType="INSIDE" width="28.0" />
            <Text fx:id="crossText" layoutX="765.0" layoutY="95.0" onMouseClicked="#closeRuleBook" opacity="0.0" strokeType="OUTSIDE" strokeWidth="0.0" text="X" />
